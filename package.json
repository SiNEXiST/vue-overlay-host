    "node-sass": "^4.9.3",
    "sass-loader": "^7.1.0",
    "tslint": "^5.11.0",
    "tslint-config-prettier": "^1.15.0",
    "typescript": "^3.0.0",
    "vue-template-compiler": "^2.5.17",
    "vuetify": "^1.2.5",
    "vuex-class": "^0.3.1"
